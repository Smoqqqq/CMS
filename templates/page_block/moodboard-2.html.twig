<div class="row moodboard">
	<div class="col-md-4">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile1') }}" style="height: 554px;">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile2') }}" style="height: 577px">
	</div>
	<div class="col-md-4">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile3') }}" style="height: 358px">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile4') }}" style="height: 254px">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile5') }}" style="height: 506px">
	</div>
	<div class="col-md-4">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile6') }}" style="height: 762px">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile7') }}" style="height: 368px">
	</div>
	<div class="col-md-6 pe-md-2" style="margin-top: -2px">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile8') }}">
	</div>
	<div class="col-md-6 ps-md-1" style="margin-top: -2px">
		<img src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile9') }}">
	</div>
</div>

<div class="swiper">
	<div class="swiper-wrapper">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile1') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile2') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile3') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile4') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile5') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile6') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile7') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile8') }}">
		<img class="swiper-slide" src="{{ vich_uploader_asset(bloc.currentBlock, 'imageFile9') }}">
	</div>

	<!-- If we need pagination -->
	<div class="swiper-pagination"></div>

	<!-- If we need navigation buttons -->
	<div class="swiper-button-prev"></div>
	<div class="swiper-button-next"></div>
</div>
